{
  "name": "termweather",
  "version": "3.1.0",
  "description": "Beautiful weather in your terminal - CLI commands + interactive TUI dashboard",
  "type": "module",
  "bin": {
    "weather": "./bin/weather.js",
    "weather-tui": "./bin/weather.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "bin",
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node ./bin/weather.js",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "ink": "^5.2.1",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.27",
    "@weather/core": "workspace:*",
    "@weather/tui": "workspace:*",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "weather",
    "cli",
    "terminal",
    "tui",
    "forecast",
    "temperature",
    "ink",
    "dashboard",
    "open-meteo"
  ],
  "author": "Aarekaz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aarekaz/Weather-Site.git"
  },
  "homepage": "https://github.com/Aarekaz/Weather-Site#readme",
  "bugs": {
    "url": "https://github.com/Aarekaz/Weather-Site/issues"
  },
  "engines": {
    "node": ">=18"
  }
}
