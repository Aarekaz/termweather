{
  "name": "@weather/tui",
  "version": "1.0.0",
  "description": "Weather TUI - interactive terminal weather dashboard",
  "type": "module",
  "bin": {
    "weather-tui": "./bin/weather-tui.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node ./bin/weather-tui.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@weather/core": "workspace:*",
    "ink": "^5.1.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "weather",
    "tui",
    "terminal",
    "dashboard",
    "ink"
  ],
  "author": "",
  "license": "MIT"
}
